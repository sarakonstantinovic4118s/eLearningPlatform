
@{
    ViewBag.Title = "E-learning";
}

@section skripte{
    <link rel="stylesheet" href="~/css/login.css">
}
<div class="reg-wrap">
    <div class="login-wrap">
        <div class="login">
            <h1>Login</h1>
            <form>
                <label>Username</label>
                <input type="text" />
                <label>Password</label>
                <input type="text" />
                <label><input type="checkbox"> Remember me</label>
                <input class="signInBtn" type="submit" value="Sign In" />
            </form>
            <a asp-controller="Learning" asp-action="Register">Don't have an account?</a>
        </div>
    </div>
</div>s